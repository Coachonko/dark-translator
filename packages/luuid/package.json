{
  "name": "@wareme/luuid",
  "description": "lexicographically sortable UUID implementation",
  "version": "3.1.0",
  "keywords": [
    "luuid"
  ],
  "os": [
    "linux"
  ],
  "cpu": [
    "x64"
  ],
  "author": "Coachonko",
  "repository": "https://github.com/Coachonko/wareme",
  "license": "NCSA",
  "main": "dist/index.js",
  "scripts": {
    "test": "bun test",
    "clean": "rm -rf ./dist",
    "build:luuid_wrapper": "v src/luuid_wrapper -o src/luuid_bin",
    "build:luuid": "bun run clean && bun build ./src/index.js --outdir ./dist --minify --sourcemap=external",
    "build:copy_bin": "cp src/luuid_bin dist/",
    "build": "bun run build:luuid_wrapper && bun run build:luuid && bun run build:copy_bin",
    "dev": "bun run build:luuid --watch"
  },
  "devDependencies": {
    "standard": "^17.1.0"
  }
}