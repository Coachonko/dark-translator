{
  "name": "@wareme/smooth-scrolling",
  "description": "High performance smooth scrolling for Dark applications",
  "version": "3.3.0",
  "keywords": [
    "smooth-scrolling",
    "Dark"
  ],
  "author": "Coachonko",
  "repository": "https://github.com/Coachonko/wareme",
  "license": "NCSA",
  "main": "dist/index.js",
  "scripts": {
    "test": "bun test",
    "clean": "rm -rf ./dist",
    "build": "bun run clean && bun build ./src/index.js --outdir ./dist --minify --sourcemap=external --external @dark-engine/core --external @wareme/raf-nexus --external @wareme/utils --external @wareme/event-emitter",
    "dev:build": "bun run clean && bun build ./src/index.js --outdir ./dist --external @dark-engine/core --external @wareme/raf-nexus --external @wareme/utils --external @wareme/event-emitter",
    "dev": "bun run dev:build --watch"
  },
  "devDependencies": {
    "standard": "^17.1.0"
  },
  "peerDependencies": {
    "@dark-engine/core": "^1.4.1",
    "@wareme/raf-nexus": "workspace:*",
    "@wareme/event-emitter": "workspace:*",
    "@wareme/utils": "workspace:*"
  }
}